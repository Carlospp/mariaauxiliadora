<h2 class="col-md-offset-5 text-primary">Datos del Paciente</h2>
<p class="col-md-offset-5" id="notice"><%= notice %></p>
<div class="container-fluid">
  <div class="row">
    <div class="col-md-6 col-md-offset-5">
<p>
  <strong>Nombre:</strong>
  <%= @patient.nombre %>
</p>

<p>
  <strong>Apellido:</strong>
  <%= @patient.apellido %>
</p>

<p>
  <strong>Dni:</strong>
  <%= @patient.dni %>
</p>

<p>
  <strong>Fecnac:</strong>
  <%= @patient.fecnac %>
</p>

<p>
  <strong>Sexo:</strong>
  <%= @patient.sexo %>
</p>

<p>
  <strong>Telefono:</strong>
  <%= @patient.telefono %>
</p>

<p>
  <strong>Celular:</strong>
  <%= @patient.celular %>
</p>

<p>
  <strong>Direccion:</strong>
  <%= @patient.direccion %>
</p>

<p>
  <strong>Email:</strong>
  <%= @patient.email %>
</p>

<p>
  <strong>Fecha Ingreso:</strong>
  <%= @patient.fecingreso %>
</p>

<p>
  <strong>Estado:</strong>
  <%= @patient.estado %>
</p>

<%= link_to 'Editar', edit_patient_path(@patient) %> |
<%= link_to 'Atras', patients_path %>
</div>
</div>
</div>